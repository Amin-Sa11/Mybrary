<h2>Search Autors</h2>
<form action="/authors" method="get">
    <label>Name</label>
    <input type="text" name= 'name' value="<%= searchOptions %>">
    <button type="submit">Search</button>
</form>
<br>
<% for (i of authors){ %>
    <div><%= i.name %></div>
    <a href="/authors/<%= i.id %> ">View</a>
    <a href="/authors/<%= i.id %>/edit ">Edit</a>
    <%- include('../partials/deleteForm.ejs', {url: `/authors/${i.id}`}) %> 
<% } %>  